{
  "manifest_version": 3,
  "name": "Font Switcher Enhanced",
  "version": "2.0.1",
  "description": "Better font selection!",
  "permissions": ["storage", "scripting", "tabs"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "run_at": "document_end"
  }]
}
